<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Sulu\Bundle\Sales\InvoiceBundle\Entity\InvoiceStatus" table="si_shipping_status">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <one-to-many field="invoice" target-entity="Sulu\Bundle\Sales\InvoiceBundle\Entity\Invoice" mapped-by="status"/>
        <one-to-many field="translations" target-entity="Sulu\Bundle\Sales\InvoiceBundle\Entity\InvoiceStatusTranslation" mapped-by="status"/>
    </entity>
</doctrine-mapping>
